<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

     <servlet>
      <servlet-name>LoginDemo</servlet-name>
       <servlet-class>LoginDemo</servlet-class>
     </servlet>
     
     <servlet>
       <servlet-name>WelcomeDemo</servlet-name>
       <servlet-class>WelcomeDemo</servlet-class>
     </servlet>
   
   
     <servlet-mapping>
       <servlet-name>LoginDemo</servlet-name>
       <url-pattern>/log</url-pattern>
     </servlet-mapping>
     
     <servlet-mapping>
       <servlet-name>WelcomeDemo</servlet-name>
       <url-pattern>/welcome</url-pattern>
     </servlet-mapping>
     
     <welcome-file-list>
       <welcome-file>index.html</welcome-file>
     </welcome-file-list>


</web-app>